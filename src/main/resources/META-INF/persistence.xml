    <jta-data-source>jdbc/wfd</jta-data-source>
    <class>com.wfd.entities.TComment</class>
    <class>com.wfd.entities.TPost</class>
    <class>com.wfd.entities.TPostRelation</class>
    <class>com.wfd.entities.TTopic</class>
    <class>com.wfd.entities.TUsers</class>
    <class>com.wfd.entities.VTopic</class>
    <class>com.wfd.entities.TCommnet</class>
